{
  "name": "triskeltrails",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:dev": "ng serve --configuration development --poll 2000",
    "start:prod": "ng serve --configuration production --poll 2000",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch",
    "test:unit": "ng test",
    "test:e2e": "cypress run",
    "cypress:open": "cypress open",
    "deploy": "npm run build:prod && gcloud app deploy app.yaml --project triskeltrails --quiet"
  },
  "type": "module",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "leaflet-gpx": "^2.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.0",
    "@types/geojson": "^7946.0.16",
    "@types/jasmine": "^6.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-gpx": "^1.3.8",
    "cypress": "^15.9.0",
    "cypress-log-to-output": "^1.1.2",
    "jasmine-core": "^6.0.1",
    "jsdom": "^27.1.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.2.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
